# EVE-OS udev rules

KERNEL=="log", MODE="0666", OWNER="root", GROUP="root"
KERNEL=="null", MODE="0666", OWNER="root", GROUP="root"
KERNEL=="zero", MODE="0666", OWNER="root", GROUP="root"
KERNEL=="full", MODE="0666", OWNER="root", GROUP="root"
KERNEL=="random", MODE="0666", OWNER="root", GROUP="root"
KERNEL=="urandom", MODE="0666", OWNER="root", GROUP="root"

KERNEL=="tpm*", MODE="0660", OWNER="root", GROUP="tpms"
KERNEL=="tpmrm*", MODE="0660", OWNER="root", GROUP="tpms"
